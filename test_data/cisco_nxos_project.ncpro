        "dst_wildcard": "",
        "dst_port": "179",
        "log": ""
      },
      {
        "seq": "20",
        "action": "permit",
        "protocol": "icmp",
        "source": "any",
        "src_wildcard": "",
        "destination": "any",
        "dst_wildcard": "",
        "dst_port": "",
        "log": ""
      },
      {
        "seq": "100",
        "action": "deny",
        "protocol": "ip",
        "source": "any",
        "src_wildcard": "",
        "destination": "any",
        "dst_wildcard": "",
        "dst_port": "",
